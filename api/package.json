  "scripts": {
    "seed": "node dist/data/seed/seeder",
    "prebuild": "rimraf dist",
    "build": "nest build --path tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start -p tsconfig.json --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compose:up": "docker-compose up",
    "compose:down": "docker-compose down --remove-orphans",
    "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js -d src/app/configs/type-orm.config.ts",
    "migration:run": "yarn typeorm migration:run",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:create": "yarn typeorm migration:create src/data/migrations/",
    "migration:revert": "yarn typeorm migration:revert",
    "schema:drop": "yarn typeorm schema:drop",
    "translations": "cp -r ../web/public/locales .",
    "cli": "CLI_PATH=./dist/cli.js npx nestjs-command",
    "cron:buttons:removepast": "yarn cli cron:buttons:removepast",
    "config:genjwt": "yarn cli config:genjwt",
    "connect-web": "cp -r ../web/src/shared/types/* src/shared/types/ && yarn translations"
  },
